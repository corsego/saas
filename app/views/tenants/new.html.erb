<% model_class = Tenant %>
<div class="card shadow">
  <div class="card-header">
    <h4>
      <%= t("helpers.titles.new").titleize %>
      <%= model_class.model_name.human.titleize %>
    </h4>
  </div>
  <div class="card-body">
    <%= render 'form', tenant: @tenant %>
  </div>
  <div class="card-footer">
    <%= link_to t("helpers.links.back").capitalize, my_tenants_path %>
  </div>
</div>